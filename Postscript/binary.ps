/bin_to_dec2
{
    counttomark 0 1 2 div
    3 -1 roll {
        2 mul
        dup 3 1 roll
        4 2 roll mul
        3 1 roll exch
        3 -1 roll
        add
        exch
    }repeat
    pop exch pop
} def

/bin_to_dec3
